/*Abdul Basit (18),bscs1(D)*/
#include<iostream>
using namespace std;
int main()
{
    int arr[10];
    cout<<"enter 10 values for array :"<<endl;
    for(int i=0;i<10;i++)
    cin>>arr[i];
    cout<<"you entered following integers in array :"<<endl;
    for(int i=0;i<10;i++)
    cout<<arr[i]<<",";
    cout<<endl;
    cout<<"elements of Array in reverse order :"<<endl;
    for(int i=9;i>=0;i--)
    cout<<arr[i]<<",";   
    return 0;
}
